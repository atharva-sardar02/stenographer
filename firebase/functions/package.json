{
  "name": "stenographer-functions",
  "version": "0.1.0",
  "description": "Firebase Cloud Functions for Stenographer",
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs');const p=require('path');if(fs.existsSync('lib/firebase')){const src='lib/firebase/functions/src';const files=fs.readdirSync(src,{recursive:true});files.forEach(f=>{const from=p.join(src,f);const to=p.join('lib',f);if(fs.statSync(from).isFile()){fs.mkdirSync(p.dirname(to),{recursive:true});fs.copyFileSync(from,to)}});fs.rmSync('lib/firebase',{recursive:true,force:true})}\"",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/vision": "^4.0.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0",
    "mammoth": "^1.11.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "private": true
}
